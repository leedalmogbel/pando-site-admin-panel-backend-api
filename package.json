{
    "name": "pando-site-admin-panel-backend-api",
    "version": "1.0.0",
    "description": "Backend API for Pando Admin Panel Site",
    "main": "app.js",
    "scripts": {
        "start": "node app.js | bunyan",
        "dev": "nodemon app.js | bunyan",
        "forever": "forever -a --uid \"pando-admin-panel-api-dev-5000\" start app.js | bunyan",
        "pm2": "pm2 start app.js --name \"pando-admin-panel-prod\" -i 2",
        "createAdminUser": "node ./scripts/create-admin-user.js",
        "softDeleteUsers": "node ./scripts/soft-delete-users.js",
        "hardDeleteUsers": "node ./scripts/hard-delete-soft-deleted-users.js",
        "createPlan": "node ./scripts/add-plan.js",
        "createMiningLogs": "node ./scripts/create-mining-logs.js",
        "fixMiningLogs": "node ./scripts/fix-passive-mining-logs.js",
        "resetMiningLogs": "node ./scripts/reset-passive-mining-logs.js",
        "resetReferrals": "node ./scripts/reset-referrals.js",
        "filteredResetReferrals": "node ./scripts/filtered-reset-referrals.js",
        "filteredResetReferralEarnings": "node ./scripts/filtered-reset-referral-earnings.js",
        "updateUsersPlansPurchasedDate": "node ./scripts/update-plans-purchased-date.js",
        "maintenance": "node ./scripts/toggle-maintenance.js",
        "withdraw": "node ./scripts/toggle-withdraw.js",
        "settings": "node ./scripts/settings.js",
        "generateKeyPair": "node ./scripts/generate-key-pair.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@bitbucket.org/shindaisuke/pando-site-admin-panel-backend-api.git"
    },
    "author": "dnet",
    "license": "ISC",
    "homepage": "https://bitbucket.org/shindaisuke/pando-site-admin-panel-backend-api#readme",
    "dependencies": {
        "aes-js": "^3.1.2",
        "axios": "^0.21.0",
        "bcrypt": "^5.0.0",
        "bluebird": "^3.7.2",
        "bunyan": "^1.8.14",
        "csv-parser": "^2.3.3",
        "csv-writer": "^1.6.0",
        "dotenv": "^8.2.0",
        "moment": "^2.27.0",
        "mongoose": "^5.10.3",
        "mongoose-currency": "^0.2.0",
        "mongoose-seed": "^0.6.0",
        "mongoose-unique-validator": "^2.0.3",
        "multer": "^1.4.2",
        "neat-csv": "^5.2.0",
        "nodemailer": "^6.4.11",
        "object-hash": "^2.0.3",
        "pbkdf2": "^3.1.1",
        "referral-code-generator": "^1.0.8",
        "restify": "^8.5.1",
        "restify-cookie-session": "^0.1.3",
        "restify-cors-middleware": "^1.1.1",
        "restify-errors": "^8.0.2",
        "restify-session": "^1.1.1",
        "uuid": "^8.3.0"
    }
}
